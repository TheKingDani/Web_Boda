@model IEnumerable<IAB_MVC_V1.Models.tblStatusPagos>

@{
    ViewBag.Title = "Index";
}

<h2>Estatus de Pagos</h2>

<p>
    @Html.ActionLink("Crear Nuevo Estatus", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Estatus de Pago
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nomStatus)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idStatusPago }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idStatusPago }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idStatusPago })
        </td>
    </tr>
}

</table>
