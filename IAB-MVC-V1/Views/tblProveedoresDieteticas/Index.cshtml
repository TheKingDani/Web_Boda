@model IEnumerable<IAB_MVC_V1.Models.tblProveedoresDieteticas>

@{
    ViewBag.Title = "Index";
}

<h2>Dieteticas</h2>

<p>
    @Html.ActionLink("Crear Nueva Dietetica", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Nombre Dietetica
        </th>
        <th>
            Nombre del Negocio
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tblDieteticas.nomDietetica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblProveedores.nomNegocio)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idProveedorDietetica }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idProveedorDietetica }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idProveedorDietetica })
        </td>
    </tr>
}

</table>
