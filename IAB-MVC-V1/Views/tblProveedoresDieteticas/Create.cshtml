@model IAB_MVC_V1.Models.tblProveedoresDieteticas

@{
    ViewBag.Title = "Create";
}

@{
    Layout = "~/Views/Shared/_Layout - Copia.cshtml";
}
<div class="content" style="margin-left: 0;">
    <h4>Opciones dietéticas</h4>
   

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

<div class="form-horizontal">

    <hr />
    <p>Seleccione las opciones dietéticas que puede atender.</p>
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        @Html.LabelFor(model => model.idDietetica, "idDietetica", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownList("idDietetica", null, htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.idDietetica, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.idProveedor, "idProveedor", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownList("idProveedor", null, htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.idProveedor, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Guardar" class="btn blue" />
        </div>
    </div>

</div>
        }

        <div>
            @Html.ActionLink("Back to List", "Index")
        </div>
    </div>