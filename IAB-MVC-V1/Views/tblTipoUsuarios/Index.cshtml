@model IEnumerable<IAB_MVC_V1.Models.tblTipoUsuarios>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo Usuario", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombreTipoUsuario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreTipoUsuario)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.tipoUsuarioID }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.tipoUsuarioID }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.tipoUsuarioID })
        </td>
    </tr>
}

</table>
