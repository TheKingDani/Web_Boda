@model IEnumerable<IAB_MVC_V1.Models.tblRespuestasProveedores>

@{
    ViewBag.Title = "Index";
}

<h2>Respuestas de Pregunatas a Proveedores</h2>

<p>
    @Html.ActionLink("Crear Nueva Respuesta", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Respuesta
        </th>
        <th>
            Pregunta
        </th>
        <th>
            Nombre del Negocio
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Respuesta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblPreguntasProveedores.Pregunta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblProveedores.nomNegocio)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idRespuestaProveedor }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idRespuestaProveedor }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idRespuestaProveedor })
        </td>
    </tr>
}

</table>
