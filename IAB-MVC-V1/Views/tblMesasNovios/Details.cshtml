@model IAB_MVC_V1.Models.tblMesasNovios

@{
    ViewBag.Title = "Details";
}

<h2>Detalles</h2>

<div>
    <h4>Mesa de Novios</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Número
        </dt>

        <dd>
            @Html.DisplayFor(model => model.numero)
        </dd>

        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tblMesasRegalos.nombre)
        </dd>

        <dt>
            Novia
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tblNovios.nombreNovia)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.idMesaNovio }) |
    @Html.ActionLink("Regresar", "Index")
</p>
