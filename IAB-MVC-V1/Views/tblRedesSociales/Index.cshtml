@model IEnumerable<IAB_MVC_V1.Models.tblRedesSociales>

@{
    ViewBag.Title = "Index";
}

<h2>Redes Sociales</h2>

<p>
    @Html.ActionLink("Crear Nuva Red Social", "Create")
</p>
<table class="table">
    <tr>
        <th>
           Red Social
        </th>
        <th>
           Estatus
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.redSocial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estatus)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idRedSocial }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idRedSocial }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idRedSocial })
        </td>
    </tr>
}

</table>
