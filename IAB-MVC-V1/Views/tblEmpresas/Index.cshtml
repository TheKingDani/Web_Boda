@model IEnumerable<IAB_MVC_V1.Models.tblEmpresas>

@{
    ViewBag.Title = "Concentrado de empresas";
}

<h2>Listado de empresas</h2>

<p>
    @Html.ActionLink("Registrar empresa", "Create")
</p>
<table class="table">
    <tr>
        <th>
           Empresa
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreEmpresa)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idEmpresa }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idEmpresa }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idEmpresa })
        </td>
    </tr>
}

</table>
