@model IAB_MVC_V1.Models.tblProductosProveedores

@{
    ViewBag.Title = "Details";
}

<h2>Detalles</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre del Producto
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombreProducto)
        </dd>

        <dt>
            Precio del Producto
        </dt>

        <dd>
            @Html.DisplayFor(model => model.precioProducto)
        </dd>

        <dt>
            Descripción
        </dt>

        <dd>
            @Html.DisplayFor(model => model.descripcion)
        </dd>

        <dt>
            Servicio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tblServiciosProveedores.idServicioProveedor)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.idProductoProveedor }) |
    @Html.ActionLink("Regresar a la lista", "Index")
</p>
