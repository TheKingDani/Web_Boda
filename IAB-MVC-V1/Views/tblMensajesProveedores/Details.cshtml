@model IAB_MVC_V1.Models.tblMensajesProveedores

@{
    ViewBag.Title = "Details";
}

<h2>Detalles</h2>

<div>
    <dl class="dl-horizontal">
        <dt>
           Titulo del Mensaje
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tituloMensaje)
        </dd>

        <dt>
            Mensaje
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Mensaje)
        </dd>

        <dt>
          Nombre del Negocio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tblProveedores.nomNegocio)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.idMensajeProveedor }) |
    @Html.ActionLink("Regresar a la Lista", "Index")
</p>
