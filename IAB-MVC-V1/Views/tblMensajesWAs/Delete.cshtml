@model IAB_MVC_V1.Models.tblMensajesWA

@{
    ViewBag.Title = "Eliminar ";
}

<h2>Eliminar</h2>

<h3>¿Estas seguro de eliminar este mensaje WA?</h3>
<div>
    <h4>Mensajes WA</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ID Tipo Invitado
        </dt>

        <dd>
            @Html.DisplayFor(model => model.idTipoInvitado)
        </dd>

        <dt>
          ID Idioma
        </dt>

        <dd>
            @Html.DisplayFor(model => model.idIdioma)
        </dd>

        <dt>
            Mensaje Inicial
        </dt>

        <dd>
            @Html.DisplayFor(model => model.mensajeInicial)
        </dd>

        <dt>
            Mensaje de Mesa de Regalos
        </dt>

        <dd>
            @Html.DisplayFor(model => model.mensajeMesaRegalos)
        </dd>

        <dt>
            Mensaje Día D
        </dt>

        <dd>
            @Html.DisplayFor(model => model.mensajeDiaD)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Elimiar" class="btn btn-default" /> |
            @Html.ActionLink("Regresar", "Index")
        </div>
    }
</div>
