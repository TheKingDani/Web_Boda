@model IEnumerable<IAB_MVC_V1.Models.tblPreguntasProveedores>

@{
    ViewBag.Title = "Index";
}

<h2>Pruguntas Proveedores</h2>

<p>
    @Html.ActionLink("Crear Nueva Pregunta", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Pregunta)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pregunta)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idPreguntaProveedor }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idPreguntaProveedor }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idPreguntaProveedor })
        </td>
    </tr>
}

</table>
