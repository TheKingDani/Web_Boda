@model IEnumerable<IAB_MVC_V1.Models.tblIntentosIngreso>

@{
    ViewBag.Title = "Index";
}

<h2>Ingreso</h2>

<p>
    @Html.ActionLink("Registrar", "Create")
</p>
<table class="table">
    <tr>
        <th>
         Usuario
        </th>
        <th>
          Contraseña
        </th>
        <th>
           Fecha Hora
        </th>
        <th>
           Ingreso
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.usuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaHora)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ingreso)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idIntentoIngreso }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.idIntentoIngreso }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idIntentoIngreso })
        </td>
    </tr>
}

</table>
