@model IAB_MVC_V1.Models.tblEmpresasOrganizadores

@{
    ViewBag.Title = "Details";
}

<h2>Detalles</h2>

<div>
    <dl class="dl-horizontal">
        <dt>
            Nombre de la Empresa
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombreEmpresa)
        </dd>

        <dt>
           Dirección
        </dt>

        <dd>
            @Html.DisplayFor(model => model.direccion)
        </dd>

        <dt>
            Telefono
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telefono)
        </dd>

        <dt>
           Pagina Web
        </dt>

        <dd>
            @Html.DisplayFor(model => model.paginaWeb)
        </dd>

        <dt>
            Nombre del Estado
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tblEstados.nombreEstado)
        </dd>

        <dt>
           Nombre del Municipio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tblMunicipios.nombreMunicipio)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.idEmpresaOrganizador }) |
    @Html.ActionLink("Regresar a la Lista", "Index")
</p>
