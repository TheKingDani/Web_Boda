@model IAB_MVC_V1.Models.tblCategoriasProveedores

@{
    ViewBag.Title = "Details";
}

<h2>Detalles</h2>

<div>
    <h4>Categorías de Proveedores</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre de la Categoría
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombreCategoria)
        </dd>

        <dt>
            Descripción
        </dt>

        <dd>
            @Html.DisplayFor(model => model.descripcion)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.idCategoriaProveedor }) |
    @Html.ActionLink("Regresar a la lista", "Index")
</p>
